function GetNthTerm_21_22_24_27(N)    
    term = 21
    for I = 1; I < N ; I++ 
        term += I        
    end for 
    return term

static int GetNthTerm_21_22_24_27(int N) 
{
    int term = 21;  //21 22 24 27 31
    int difference = 1;//1 2 3 4 5
    for (int I = 1; I <= N; I++)
    {
        if( I == N )
        {
            break;
        }
        //
        term = term + difference;
        difference = difference + 1;
    }
    return term;
}
